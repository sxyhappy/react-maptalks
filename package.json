{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start:react-maptalks": "cd packages/react-maptalks && yarn run start",
    "start:react-core": "cd packages/core && yarn run start",
    "build:react-maptalks": "cd packages/react-maptalks && yarn run build",
    "build:react-core": "cd packages/core && yarn run build",
    "build": "yarn run build:react-core && yarn run build:react-maptalks",
    "run:doc": "cd packages/docs && yarn run start"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/react": "^17.0.6",
    "maptalks": "^0.49.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.49.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  }
}
